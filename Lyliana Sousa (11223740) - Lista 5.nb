     RowBox[{
      SubscriptBox["L", "n"], "(", "x", ")"}]}], "=", " ", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{"n", "!"}]], 
      SuperscriptBox[
       RowBox[{"D", "(", 
        RowBox[{"D", "-", "1"}], ")"}], "n"], 
      SuperscriptBox["x", "n"]}], " ", "=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox["1", 
