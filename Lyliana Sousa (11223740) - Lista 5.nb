Cell[TextData[{
 "Vou demonstrar agora uma nova forma de escrever a formula de Rodrigues  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["L", "n"], "(", "x", ")"}], "=", " ", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"n", "!"}]], 
     SuperscriptBox["e", "x"], 
     RowBox[{
      FractionBox[
       SuperscriptBox["\[DifferentialD]", "n"], 
       RowBox[{"\[DifferentialD]", 
        SuperscriptBox["x", "n"]}]], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x", "n"], 
        SuperscriptBox["e", 
         RowBox[{"-", "x"}]]}], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f42d5d47-8485-42f1-8bb4-53eff7f523f0"],
 ". Para isso, utilizarei o operador ",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "=", 
    FractionBox["\[DifferentialD]", 
     RowBox[{"\[DifferentialD]", "x"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cd1353ae-b32d-4c86-a314-0740143d3e05"],
 " e ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    SuperscriptBox["e", 
     RowBox[{"-", "x"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "20bde81d-4d11-4f02-a7a9-a4c147f91a6e"],
 ". Assim:\n\[RightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["D", "n"], "(", 
     SuperscriptBox["ye", 
      RowBox[{"-", "x"}]], ")"}], "=", " "}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0da04a75-db15-491a-900a-3b1f2393363f"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["D", 
      RowBox[{"n", "-", "1"}]], "[", 
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["ye", 
        RowBox[{"-", "x"}]]}], " ", "+", " ", 
      RowBox[{
       SuperscriptBox["e", 
        RowBox[{"-", "x"}]], "Dy"}]}], "]"}], "="}], TraditionalForm]],
  ExpressionUUID->"b7170299-834d-4abc-b9cc-a66ac72f449a"],
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{
      SuperscriptBox["D", 
       RowBox[{"n", "-", "1"}]], "[", 
      RowBox[{
       SuperscriptBox["e", 
        RowBox[{"-", "x"}]], " ", 
       RowBox[{"(", 
        RowBox[{"D", "-", "1"}], ")"}], "y"}], "]"}], "="}]}], 
   TraditionalForm]],ExpressionUUID->"3d3d9c17-c6cf-4919-b727-eca28bc4a8f8"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["D", 
