        SuperscriptBox["x", "n"]}], " ", "-", 
       RowBox[{
        FractionBox["1", 
         RowBox[{"n", "!"}]], 
        SuperscriptBox[
         RowBox[{"D", "(", 
          RowBox[{"D", "-", "1"}], ")"}], 
         RowBox[{"n", "-", "1"}]], 
        SuperscriptBox["x", "n"]}]}], " ", "=", "  "}]}]}], TraditionalForm]],
  
  FormatType->"TraditionalForm",ExpressionUUID->
  "fd7e4487-84b8-4835-9a39-ce5ee6b81ed6"],
 "\n  ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
