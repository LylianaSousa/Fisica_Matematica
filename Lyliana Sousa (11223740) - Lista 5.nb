    RowBox[{
     RowBox[{
      RowBox[{"D", "(", 
       RowBox[{"D", "-", "1"}], ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"D", "-", "1"}], ")"}], 
       RowBox[{"n", "-", "1"}]]}], " ", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"D\.b2", "-", "D"}], ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"D", "-", "1"}], ")"}], 
       RowBox[{"n", "-", "1"}]], " "}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "af4e3865-5cc3-4baf-a511-d1b5583d86be"],
 ". Portanto,\n ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["\[DifferentialD]", 
