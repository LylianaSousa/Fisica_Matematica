Cell[TextData[{
 "Mostrando que ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{"-", "x"}]], 
      SuperscriptBox["x", "m"], 
      SubscriptBox["L", "n"], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "=", " ", "0"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "26646ff1-0fe2-4ac3-bb48-c751c77b0142"],
 ", para m<n. Tenho que:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{"-", "x"}]], 
      SuperscriptBox["x", "m"], 
      SubscriptBox["L", "n"], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
      RowBox[{
       FractionBox["1", 
        RowBox[{"n", "!"}]], 
       SuperscriptBox["x", "m"], 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[DifferentialD]", "n"], 
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["x", "n"]}]], 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["x", "n"], 
          SuperscriptBox["e", 
           RowBox[{"-", "x"}]]}], ")"}]}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "=", " ", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "n"], 
      FractionBox[
       RowBox[{"m", "!"}], 
       RowBox[{"n", "!"}]], " ", 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[DifferentialD]", 
          RowBox[{"n", "-", "m"}]], 
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["x", 
           RowBox[{"n", "-", "m"}]]}]], 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["x", "m"], 
          SuperscriptBox["e", 
           RowBox[{"-", "x"}]]}], ")"}]}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3a693e24-f26e-48df-8451-a7fda1492400"],
 ".\nVou fazer uma integra\[CCedilla]\[ATilde]o por partes, onde: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{"u", 
      RowBox[{"\[DifferentialD]", "v"}]}]}], "=", 
    RowBox[{"uv", "-", 
     RowBox[{"\[Integral]", 
      RowBox[{"v", 
       RowBox[{"\[DifferentialD]", "u"}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8be282a7-b6cf-428d-a288-d8feeb61c9a0"],
 " para u = 1, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"dv", "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", 
       RowBox[{"n", "-", "m"}]], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["x", 
        RowBox[{"n", "-", "m"}]]}]], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["x", "m"], 
       SuperscriptBox["e", 
        RowBox[{"-", "x"}]]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ac113033-cc01-499d-a312-4d445a951c0b"],
 ", du = 0, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", 
       RowBox[{"n", "-", "m", "-", "1"}]], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["x", 
        RowBox[{"n", "-", "m", "-", "1"}]]}]], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["x", "m"], 
       SuperscriptBox["e", 
        RowBox[{"-", "x"}]]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "af5f8ab2-f16b-4fa9-8768-73bcf06c7c32"],
 ". Assim, usando a regra de Leibniz, tenho:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{"-", "x"}]], 
      SuperscriptBox["x", "m"], 
      SubscriptBox["L", "n"], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        SuperscriptBox["\[DifferentialD]", 
         RowBox[{"n", "-", "m", "-", "1"}]], 
        RowBox[{"\[DifferentialD]", 
         SuperscriptBox["x", 
          RowBox[{"n", "-", "m", "-", "1"}]]}]], 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["x", "m"], 
         SuperscriptBox["e", 
          RowBox[{"-", "x"}]]}], ")"}]}], 
      SubsuperscriptBox["|", "0", "\[Infinity]"]}], "=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"j", "=", "0"}], "n"], 
        RowBox[{
         RowBox[{"(", GridBox[{
            {"n"},
            {"j"}
           }], ")"}], 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            SubscriptBox["\[PartialD]", "x"], "j"], 
           SuperscriptBox["x", "m"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            SubscriptBox["\[PartialD]", "x"], 
            RowBox[{"n", "-", "j"}]], 
           SuperscriptBox["e", 
            RowBox[{"-", "x"}]]}], ")"}]}]}], 
       SubsuperscriptBox["|", "0", "\[Infinity]"]}], "="}]}]}], 
   TraditionalForm]],ExpressionUUID->"a86eb92d-74f3-4334-923f-e43bf8abe532"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], "n"], 
       RowBox[{
        RowBox[{"(", GridBox[{
           {"n"},
           {"j"}
          }], ")"}], 
        FractionBox[
         RowBox[{"n", "!"}], 
         RowBox[{"j", "!"}]], 
        SuperscriptBox[
         RowBox[{
          SuperscriptBox["x", 
           RowBox[{"m", "-", "j"}]], "(", 
          RowBox[{"-", "1"}], ")"}], "j"], 
        SuperscriptBox["e", 
         RowBox[{"-", "x"}]]}]}], 
      SubsuperscriptBox["|", "0", "\[Infinity]"]}], "=", "0"}]}], 
   TraditionalForm]],ExpressionUUID->"6d6cfd94-1658-414a-aef7-d32d6ea2fe6c"]
}], "Text",
