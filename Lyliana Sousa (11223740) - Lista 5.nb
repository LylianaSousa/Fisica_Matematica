Cell[TextData[{
 "Usaremos o mesmo principio para a EDO ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"xy", "'"}], "-", "y"}], "=", "x\.b2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "90cc3a01-469b-4b65-9ee4-f492b3c7f8a1"],
 "\n ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"xy", "'"}], "-", "y"}], "=", "x\.b2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "dae73a25-ffe6-4d31-9546-92e6c0c3ea1b"],
 "\[DoubleLeftRightArrow]  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"x", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], "\[Infinity]"], 
       RowBox[{"j", " ", 
        SubscriptBox["a", "j"], 
        SuperscriptBox["x", 
         RowBox[{"j", "-", "1"}]]}]}]}], "-", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"j", "=", "0"}], "\[Infinity]"], 
      RowBox[{
       SubscriptBox["a", "j"], 
       SuperscriptBox["x", "j"]}]}]}], "=", "x\.b2", " "}], TraditionalForm]],
  
  FormatType->"TraditionalForm",ExpressionUUID->
  "9c1e8f90-74b3-48fc-9b00-bd553f8549a0"],
 "\[DoubleLeftRightArrow]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"j", " ", 
         SubscriptBox["a", "j"]}], "-", 
        SubscriptBox["a", "j"]}], "}"}], 
      SuperscriptBox["x", "j"]}]}], "=", "x\.b2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "086d295b-bc6b-4f78-8f84-f3ad259a7933"],
 "\n Para j=0: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"j", " ", 
      SubscriptBox["a", "j"]}], "-", 
     SubscriptBox["a", "j"]}], "=", 
    RowBox[{
     RowBox[{"0", "\[DoubleRightArrow]", 
      SubscriptBox["a", "0"]}], "=", "0", " "}]}], TraditionalForm]],
  ExpressionUUID->"f7b393ce-2892-41df-bdba-bc3a768edc8d"],
 "\n Para j=1: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"j", " ", 
      SubscriptBox["a", "j"]}], "-", 
     SubscriptBox["a", "j"]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["a", "1"], "-", 
      SubscriptBox["a", "1"]}], "=", 
     RowBox[{
      RowBox[{"0", "\[DoubleRightArrow]", 
       SubscriptBox["a", "j"]}], "=", 
      RowBox[{
       SubscriptBox["c", "1"], "=", 
       RowBox[{"constante", " ", "qualquer", " "}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"fa789ed2-409d-440e-a711-be2349f32be5"],
 "\n Para j=2: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"j", " ", 
      SubscriptBox["a", "j"]}], "-", 
     SubscriptBox["a", "j"]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"2", 
       SubscriptBox["a", "2"]}], "-", 
      SubscriptBox["a", "2"]}], "=", 
     RowBox[{
      RowBox[{"1", "\[DoubleRightArrow]", 
       SubscriptBox["a", "2"]}], "=", "1", " "}]}]}], TraditionalForm]],
  ExpressionUUID->"e41113c8-7f98-46e7-9aad-109e08ccb17a"],
 "\n Para j>2: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"j", " ", 
      SubscriptBox["a", "j"]}], "-", 
     SubscriptBox["a", "j"]}], "=", 
    RowBox[{
     RowBox[{"0", "\[DoubleRightArrow]", 
      SubscriptBox["a", "j"]}], "=", "0", " "}]}], TraditionalForm]],
  ExpressionUUID->"4390adf0-486e-4efa-8749-688021c4d4aa"],
 "\n Assim, nossa solu\[CCedilla]\[ATilde]o ser\[AAcute]: ",
 Cell[BoxData[
