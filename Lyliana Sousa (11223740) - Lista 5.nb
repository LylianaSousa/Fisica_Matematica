Cell[TextData[{
 "Usarei a regra de Leibniz,",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", "n"], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["x", "n"]}]], 
     RowBox[{"(", 
      RowBox[{"u", ".", "v"}], ")"}]}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "0"}], "n"], 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"n"},
         {"j"}
        }], ")"}], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["\[PartialD]", "x"], "j"], "u"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["\[PartialD]", "x"], 
         RowBox[{"n", "-", "j"}]], "v"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"d1005f93-f81f-4b6d-b229-8609c41442c3"],
 " para ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {"n"},
       {"j"}
      }], ")"}], "=", 
    FractionBox[
     RowBox[{"n", "!"}], 
     RowBox[{
      RowBox[{"j", "!"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"n", "-", "j"}], ")"}], "!"}]}]]}], TraditionalForm]],
  ExpressionUUID->"6255181f-a223-4e3d-9aaf-c1bacf083bed"],
 " , para chegar na forma polinomial do polin\[OAcute]mio de Laguerre ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["L", "n"], "(", "x", ")"}], "=", " ", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"n", "!"}]], 
     SuperscriptBox["e", "x"], 
     RowBox[{
      FractionBox[
       SuperscriptBox["\[DifferentialD]", "n"], 
       RowBox[{"\[DifferentialD]", 
        SuperscriptBox["x", "n"]}]], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x", "n"], 
        SuperscriptBox["e", 
         RowBox[{"-", "x"}]]}], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bbcfda9b-bc6a-47ca-a8a0-d2849afe1a0b"],
 ". Para isso, terei:\n\[RightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["\[PartialD]", "x"], "j"], 
     SuperscriptBox["x", "n"]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"n", "!"}], 
      RowBox[{"j", "!"}]], 
     SuperscriptBox["x", "j"]}]}], TraditionalForm]],ExpressionUUID->
  "c4d274eb-3ee3-4e70-b94a-eb869878ec1a"],
 "\n\[RightArrow]",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["\[PartialD]", "x"], 
      RowBox[{"n", "-", "j"}]], 
     SuperscriptBox["e", 
      RowBox[{"-", "x"}]]}], "=", " ", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "j"], 
     SuperscriptBox["e", 
      RowBox[{"-", "x"}]]}]}], TraditionalForm]],ExpressionUUID->
  "4d338c65-7c8c-4dde-9274-d073f566dc0a"],
 "\n\[RightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", "n"], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["x", "n"]}]], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["x", "n"], 
       SuperscriptBox["e", 
        RowBox[{"-", "x"}]]}], ")"}]}], " ", "=", " ", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "0"}], "n"], 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"n"},
         {"j"}
        }], ")"}], 
      FractionBox[
       RowBox[{"n", "!"}], 
       RowBox[{"j", "!"}]], 
      SuperscriptBox["x", "j"], " ", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "j"], 
      SuperscriptBox["e", 
       RowBox[{"-", "x"}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "bb3f11ee-1d91-4e53-b7d4-61bb0cf0cfd3"],
 "\nAssim, tenho que: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["L", "n"], "(", "x", ")"}], "=", " ", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "0"}], "n"], 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"n"},
         {"j"}
        }], ")"}], 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"-", "x"}], ")"}], "j"], 
       RowBox[{"j", "!"}]], " "}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bddbb04b-ca43-4a20-9ac7-a8a730429e89"]
}], "Text",
 CellChangeTimes->{{3.817461245542726*^9, 3.817461409940279*^9}, {
