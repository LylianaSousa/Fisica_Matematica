Cell[TextData[{
 "Para N = 5, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Phi]", " ", "=", " ", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["\[Phi]", "1"], ",", " ", "...", " ", ",", " ", 
      SubscriptBox["\[Phi]", "N"]}], " ", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e42c30b4-46df-4085-807d-e4e83822fe80"],
 " , ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Phi]", 
     RowBox[{"N", "+", "1"}]], "=", 
