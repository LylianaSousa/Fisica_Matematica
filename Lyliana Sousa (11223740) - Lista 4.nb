Cell[TextData[{
 "Nessa quest\[ATilde]o, calcularei agora a m\[EAcute]dia do momento \
\[LeftAngleBracket]p\[RightAngleBracket], para ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "=", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]\[HBar]"}], 
     SubscriptBox["\[PartialD]", "x"]}]}], TraditionalForm]],ExpressionUUID->
  "add7f2d9-3e04-450e-92f1-1a5eda4f02f0"],
 ", e a m\[EAcute]dia da posi\[CCedilla]\[ATilde]o \[LeftAngleBracket]x\
\[RightAngleBracket].\n\[RightArrow] Calculando \[LeftAngleBracket]x\
\[RightAngleBracket]:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "x", "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], "x"}], "|", 
     RowBox[{"\[Psi]", "(", "x", ")"}], "|", 
     RowBox[{"\.b2", 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"f136c64b-f051-433b-9d5b-fbab0e352a7e"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{"-", "a"}], "a"], 
    RowBox[{
     FractionBox["3", 
      RowBox[{"4", "a\.b3"}]], 
     RowBox[{"(", 
      RowBox[{"a\.b2x", "-", "x\.b3"}], ")"}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "2e85d0c6-21c9-4922-be2f-01d295a54ec9"],
 " =  ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox[
    RowBox[{
     FractionBox["3", 
      RowBox[{"4", "a\.b3"}]], "[", 
     RowBox[{
      RowBox[{"a\.b2", 
       FractionBox["x\.b2", "2"]}], "-", 
      FractionBox[
       SuperscriptBox["x", "4"], "4"]}], "]"}], 
    RowBox[{"-", "a"}], "a"], TraditionalForm]],ExpressionUUID->
  "d1476c1a-bd16-4593-818c-4cc4584921f3"],
 "= 0 \[Implies]",
 StyleBox[" \[LeftAngleBracket]x\[RightAngleBracket] = 0 ",
  Background->RGBColor[1, 1, 0]],
 " ",
 StyleBox["(4.3)",
  FontColor->RGBColor[1, 0, 0]],
 "\n\[RightArrow] Calculando \[LeftAngleBracket]p\[RightAngleBracket]: Antes \
de come\[CCedilla]ar a calculo, vou calcular a derivada por x de \[Psi]. Al\
\[EAcute]m disso, como \[Psi] \[EAcute] uma fun\[CCedilla]\[ATilde]o real ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Psi]", "*"], "=", "\[Psi]"}], TraditionalForm]],
  ExpressionUUID->"4eaded96-0290-4e13-95fd-59443bca9446"],
 "\n\[RightTeeArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", "x"}]], "\[Psi]"}], "=", 
    TagBox[GridBox[{
       {"\[Piecewise]", GridBox[{
          {
           RowBox[{"-", 
            FractionBox[
             RowBox[{"3", "x"}], 
             RowBox[{"2", "a\.b3"}]]}], 
           RowBox[{",", " ", 
            RowBox[{
             RowBox[{"-", "a"}], " ", "\[LessEqual]", " ", "x", " ", 
             "\[LessEqual]", " ", "a"}]}]},
          {"0", 
           RowBox[{",", " ", 
            RowBox[{"caso", " ", "contr\[AAcute]rio"}]}]}
         },
         AllowedDimensions->{2, Automatic},
         Editable->True,
         GridBoxAlignment->{"Columns" -> {{Left}}, "Rows" -> {{Baseline}}},
         GridBoxItemSize->{"Columns" -> {{Automatic}}, "Rows" -> {{1.}}},
         GridBoxSpacings->{"Columns" -> {
             Offset[0.27999999999999997`], {
              Offset[0.84]}, 
             Offset[0.27999999999999997`]}, "Rows" -> {
             Offset[0.2], {
              Offset[0.4]}, 
             Offset[0.2]}},
         Selectable->True]}
      },
      GridBoxAlignment->{"Columns" -> {{Left}}, "Rows" -> {{Baseline}}},
      GridBoxItemSize->{"Columns" -> {{Automatic}}, "Rows" -> {{1.}}},
      GridBoxSpacings->{"Columns" -> {
          Offset[0.27999999999999997`], {
           Offset[0.35]}, 
          Offset[0.27999999999999997`]}, "Rows" -> {
          Offset[0.2], {
           Offset[0.4]}, 
          Offset[0.2]}}],
     "Piecewise",
     DeleteWithContents->True,
     Editable->False,
     SelectWithContents->True,
     Selectable->False]}], TraditionalForm]],ExpressionUUID->
  "a157acd6-ae54-43be-8eff-15af4a218a09"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "p", "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]\[HBar]"}], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
      RowBox[{
       SuperscriptBox["\[Psi]", "*"], 
       RowBox[{
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", "x"}]], "\[Psi]"}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"1985e50d-a284-490e-868f-b9850e6ebf50"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ImaginaryI]\[HBar]", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "a"}], "a"], 
     RowBox[{
      FractionBox["9", 
       RowBox[{"8", 
        SuperscriptBox["a", "6"]}]], 
      RowBox[{"(", 
       RowBox[{"a\.b2x", "-", "x\.b3"}], ")"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"06aa125e-c8d9-4252-b77e-ac5880ccdd9a"],
 " =  ",
