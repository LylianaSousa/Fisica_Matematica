Cell[TextData[{
 "Utilizarei a  transformada de Fourier para calcular a solu\[CCedilla]\
\[ATilde]o da equa\[CCedilla]\[ATilde]o diferencial ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"\[Del]", "\.b2"}]}], " ", "+", " ", "m\.b2"}], ")"}], 
     RowBox[{"G", "(", "\[DoubleStruckR]", ")"}]}], "=", " ", 
    RowBox[{"\[Delta]", "(", "\[DoubleStruckR]", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a6c74306-17f3-467f-8024-bf371d5806ca"],
 " ",
 StyleBox["(5.1)",
  FontColor->RGBColor[1, 0, 0]],
 " e utilizarei alguns conceitos vistos em aula para facilitar o encontro da \
solu\[CCedilla]\[ATilde]o.\n\[RightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"\[Del]", "\.b2"}]}], " ", "+", " ", "m\.b2"}], ")"}], 
     RowBox[{"G", "(", "\[DoubleStruckR]", ")"}]}], "=", " ", 
    RowBox[{"\[Delta]", "(", "\[DoubleStruckR]", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "aac39417-a6b6-4ecd-8ec6-ff97ba0528ee"],
 " \[RightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      RowBox[{"\[Del]", 
       RowBox[{"\.b2G", "(", "\[DoubleStruckR]", ")"}]}]}], " ", "+", " ", 
     RowBox[{"m\.b2G", "(", "\[DoubleStruckR]", ")"}]}], "=", " ", 
    RowBox[{"\[Delta]", "(", "\[DoubleStruckR]", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3f641d03-f517-40eb-b3af-3d68459c4e9b"],
 " \[Rule] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      RowBox[{
       SubsuperscriptBox["\[Integral]", 
        RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
       RowBox[{
        FractionBox[
         RowBox[{"\[DifferentialD]", "\.b3\[DoubleStruckR]"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", "\[Pi]"}], ")"}], 
          RowBox[{"3", "/", "2"}]]], " ", 
        SuperscriptBox["\[ScriptE]", 
         RowBox[{"-", 
          RowBox[{
          "\[ImaginaryI]\[DoubleStruckK]", ".", "\[DoubleStruckR]"}]}]], 
        RowBox[{"\[Del]", 
         RowBox[{"\.b2G", "(", "\[DoubleStruckR]", ")"}]}]}]}]}], " ", "+", 
     " ", 
     RowBox[{"m\.b2", 
      RowBox[{
       SubsuperscriptBox["\[Integral]", 
        RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
       RowBox[{
        FractionBox[
         RowBox[{"\[DifferentialD]", "\.b3\[DoubleStruckR]"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", "\[Pi]"}], ")"}], 
          RowBox[{"3", "/", "2"}]]], " ", 
        SuperscriptBox["\[ScriptE]", 
         RowBox[{"-", 
          RowBox[{
          "\[ImaginaryI]\[DoubleStruckK]", ".", "\[DoubleStruckR]"}]}]], 
        RowBox[{"G", "(", "\[DoubleStruckR]", ")"}]}]}]}]}], "=", " ", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
     RowBox[{
      FractionBox[
       RowBox[{"\[DifferentialD]", "\.b3\[DoubleStruckR]"}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"2", "\[Pi]"}], ")"}], 
        RowBox[{"3", "/", "2"}]]], " ", 
      SuperscriptBox["\[ScriptE]", 
       RowBox[{"-", 
        RowBox[{"\[ImaginaryI]\[DoubleStruckK]", ".", "\[DoubleStruckR]"}]}]], 
      RowBox[{"\[Delta]", "(", "\[DoubleStruckR]", ")"}]}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "321771c4-aeaf-40a6-a7b0-e8a14b0ca433"],
 "\nNo espa\[CCedilla]o de Fourier, eu tenho que  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", 
     RowBox[{"\[Del]", "\.b2"}]}], " ", "=", 
    RowBox[{"|", "\[DoubleStruckK]", "|", "\.b2"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "382aab60-bff2-4d7d-9929-f0e8eb35cbd1"],
 " ",
 StyleBox["(5.2)",
  FontColor->RGBColor[1, 0, 0]],
 " e ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["\[Delta]", "~"], "(", "\[DoubleStruckK]", ")"}], " ", "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
      RowBox[{
       FractionBox[
        RowBox[{"\[DifferentialD]", "\.b3\[DoubleStruckR]"}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", "\[Pi]"}], ")"}], 
         RowBox[{"3", "/", "2"}]]], " ", 
       SuperscriptBox["\[ScriptE]", 
        RowBox[{"-", 
         RowBox[{
         "\[ImaginaryI]\[DoubleStruckK]", ".", "\[DoubleStruckR]"}]}]], 
       RowBox[{"\[Delta]", "(", "\[DoubleStruckR]", ")"}]}]}], "=", 
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}], 
       RowBox[{"3", "/", "2"}]]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3d5b708e-c9e7-4d37-a20c-8a7709bb17dd"],
 " ",
 StyleBox["(5.3)",
  FontColor->RGBColor[1, 0, 0]],
 ". Assim, eu tenho:\n\[RightArrow]  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"|", "\[DoubleStruckK]", "|", 
     RowBox[{
      RowBox[{"\.b2", 
