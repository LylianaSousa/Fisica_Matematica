      RowBox[{
       SubscriptBox["u", "t"], "(", 
       RowBox[{"x", ",", "t"}], ")"}]}]}], " ", "="}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cfc90aaf-7aa5-4aad-b434-726f77f671f5"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
     RowBox[{
      FractionBox[
       RowBox[{"\[DifferentialD]", "x"}], 
       SqrtBox[
        RowBox[{"2", "\[Pi]"}]]], " ", 
      SuperscriptBox["\[ScriptE]", 
       RowBox[{"-", "\[ImaginaryI]kx"}]], 
      RowBox[{
       FractionBox["\[DifferentialD]", 
        RowBox[{"\[DifferentialD]", "t"}]], 
       RowBox[{"u", "(", 
        RowBox[{"x", ",", "t"}], ")"}]}]}]}], " ", "="}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "22c27d15-a63e-44a9-8dcf-5e2b96859b2d"],
